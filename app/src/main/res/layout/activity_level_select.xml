<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="comguguxmathguxgu.httpsgithub.guxgu.LevelSelectActivity">

    <Button
        android:id="@+id/level1"
        android:onClick="onLevel1Clicked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="level 1"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/level2"
        app:layout_constraintRight_toRightOf="@+id/level3"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/level3"
        app:layout_constraintTop_toTopOf="@+id/level2"/>

    <Button
        android:id="@+id/level2"
        android:onClick="onLevel2Clicked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="level 2"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="87dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="65dp"
        app:layout_constraintTop_toTopOf="parent"/>

    <Button
        android:id="@+id/level3"
        android:onClick="onLevel3Clicked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="level 3"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/level5"
        android:layout_marginStart="51dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="58dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <Button
        android:id="@+id/level4"
        android:onClick="onLevel4Clicked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="level 4"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/level2"
        android:layout_marginTop="56dp"
        app:layout_constraintTop_toBottomOf="@+id/level2"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/level2"/>

    <Button
        android:id="@+id/level5"
        android:onClick="onLevel5Clicked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="level 5"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/level7"
        app:layout_constraintRight_toRightOf="@+id/level3"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="59dp"
        app:layout_constraintLeft_toLeftOf="@+id/level3"/>

    <Button
        android:id="@+id/level6"
        android:onClick="onLevel6Clicked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="level 6"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/level4"
        android:layout_marginTop="58dp"
        app:layout_constraintTop_toBottomOf="@+id/level4"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/level4"/>

    <Button
        android:id="@+id/level7"
        android:onClick="onLevel7Clicked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="level 7"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/level9"
        app:layout_constraintRight_toRightOf="@+id/level5"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="53dp"
        app:layout_constraintLeft_toLeftOf="@+id/level5"/>

    <Button
        android:id="@+id/level8"
        android:onClick="onLevel8Clicked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="level 8"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/level6"
        android:layout_marginTop="63dp"
        app:layout_constraintTop_toBottomOf="@+id/level6"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/level6"/>

    <Button
        android:id="@+id/level9"
        android:onClick="onLevel9Clicked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="level 9"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/level7"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="128dp"
        app:layout_constraintLeft_toLeftOf="@+id/level7"/>
</android.support.constraint.ConstraintLayout>
